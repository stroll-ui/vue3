{
  "name": "@stroll/vue3-ui",
  "version": "0.0.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode pro",
    "buildUI": "vite build --mode buildUI",
    "build:package": "pnpm run gen-version && pnpm run clean && tsc -b --force ./tsconfig.esm.json && node ./scripts/pre-build/pre-cjs-build.js && tsc -b --force ./tsconfig.cjs.json && node scripts/post-build && rimraf {es,lib}/*.tsbuildinfo",
    "clean": "rimraf lib es",
    "gen-version": "node scripts/gen-version",
    "preview": "vite preview",
    "UI": "rollup -c"
  },
  "dependencies": {
    "naive-ui": "^2.26.4",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/parser": "^7.17.3",
    "@babel/preset-env": "^7.16.7",
    "@css-render/plugin-bem": "^0.15.8",
    "@css-render/vue3-ssr": "^0.15.8",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-node-resolve": "^13.1.2",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^17.0.5",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "@vue/babel-preset-jsx": "^1.2.4",
    "css-render": "^0.15.8",
    "date-fns": "^2.28.0",
    "highlight.js": "^11.3.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "rimraf": "^3.0.2",
    "rollup": "^2.62.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "seemly": "^0.3.3",
    "typescript": "^4.4.4",
    "vdirs": "^0.1.7",
    "vite": "^2.7.2",
    "vite-plugin-inspect": "^0.3.11",
    "vooks": "^0.2.12",
    "vue-router": "^4.0.12",
    "vue-tsc": "^0.29.8",
    "vueuc": "^0.4.19"
  }
}
